{
    "dotnet.defaultSolution": "voxel-command.sln",
    "files.exclude": {
        // General
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.user": true,
        "**/*.suo": true,
        "**/*.userprefs": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitmodules": true,
        "**/.vs": true,
        "**/.vsconfig": true,
        "**/obj/": true,
        // Node
        "node_modules/": true,
        // Nx
        ".nx/": true,
        "dist/": true,
        "tmp/": true,
        // Media and Assets (General)
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpeg": true,
        "**/*.jpg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.wav": true,
        "**/*.mid": true,
        "**/*.midi": true,
        // Unity Specific
        "**/*.3ds": true,
        "**/*.asset": true,
        "**/*.booproj": true,
        "**/*.cubemap": true,
        "**/*.fbx": true,
        "**/*.flare": true,
        "**/*.lxo": true,
        "**/*.ma": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.obj": true,
        "**/*.pidb": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "**/*.unityproj": true,
        "**/apps/*/Build/": true,
        "**/apps/*/hidden_changes.conf": true,
        "**/apps/*/ignore.conf": true,
        "**/apps/*/Library/": true,
        "**/apps/*/Logs/": true,
        "**/apps/*/Packages/packages-lock.json": true,
        "**/apps/*/ProjectSettings/": true,
        "**/apps/*/Recordings/**": true,
        "**/apps/*/Temp/": true,
        "**/apps/*/UserSettings/": true,
        "**/apps/*/Assets/AddressableAssetsData/": true,
        "**/apps/*/Assets/StreamingAssets/": true,
        "**/apps/*/Assets/_Game/Art/": true,
        "**/apps/*/Assets/_Game/Resources/": true,
        "**/apps/*/Assets/_Game/Materials/": true,
        "**/apps/*/Assets/_Game/Animations/": true,
        "**/apps/*/Assets/_Game/Audio/": true,
        "**/apps/*/Assets/_Game/Fonts/": true
    },
    "dotnet.preferCSharpExtension": true,
    "explorer.excludeGitIgnore": false
}
